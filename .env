MYDDLEWARE_VERSION=4.2.3-2

APP_SECRET=Thissecretisnotsosecretchangeit
APP_ENV=prod
APP_DEBUG=false
# MAILER_URL=null://

SENSITIVE_FIELDS=password,accesskey,accesstoken,token,useraccesstoken,tokensecret,consumerkey,consumersecret,apikey,apiuser,apikeysecret,clientsecret,hashkey,authid,tenant_id,client_id,client_secret

###> Delivery Address ###
DELIVERY_ADDRESS=example@email.com
###< Delivery Address ###

COMPOSER_MEMORY_LIMIT=-1

