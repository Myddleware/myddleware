<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Myddleware{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('login') }}
            <link rel="stylesheet" href="{{ asset('css/two-factor-auth.css') }}">
        {% endblock %}
            {% block css %}{% endblock css %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body{% if body_class is defined %} class="{{ body_class }}"{% endif %}>
        <style type="text/css"> 
            {% block cssin %}{% endblock cssin %}
        </style>

        {% block body %}
        	{% block arianne %}Myddleware{% endblock %}
        {% endblock %}
        
        {% block javascripts %}
            <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
            {{ encore_entry_script_tags('login') }}
        {% endblock %}
    </body>
</html>
