PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
# */5 * * * * root php /var/www/html/bin/console myddleware:jobScheduler --env=background >> /var/log/cron.log 2>&1
# * * * * * root echo "1 min" >> /var/log/cron.log 2>&1
# */5 * * * * root echo "5 min" >> /var/log/cron.log 2>&1
*/5 * * * * root sh /var/www/html/myddleware-cron.sh 2>&1
# Don't remove the empty line at the end of this file. It is required to run the cron job
